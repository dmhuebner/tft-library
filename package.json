{
  "name": "tft-library-app",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 8080",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-lib": "ng build --prod tft-library",
    "watch-lib": "ng build --prod tft-library --watch",
    "work": "bash ./scripts/work.sh",
    "npm-pack": "cd dist/tft-library && npm pack",
    "package": "npm run build-lib && npm run npm-pack && npm run schematics:build",
    "release": "bash ./scripts/release.sh",
    "schematics:build": "tsc -p projects/tft-library/tsconfig.schematics.json",
    "compodoc:lib": "compodoc -p projects/tft-library/tsconfig.lib.json -s -o -w",
    "compodoc:app": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -s --port 8081"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/fire": "^5.1.0",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "angular-instantsearch": "^2.2.2",
    "core-js": "^2.5.4",
    "firebase": "5.5.3",
    "hammerjs": "^2.0.8",
    "lodash-es": "^4.17.10",
    "rxjs": "^6.5.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.0",
    "@angular-devkit/build-ng-packagr": "^0.800.0",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@compodoc/compodoc": "^1.1.9",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.9.5",
    "codelyzer": "~5.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.4.0",
    "protractor": "^5.4.1",
    "ts-node": "~5.0.1",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.3",
    "tslint": "~5.9.1",
    "typescript": "~3.1.6",
    "webpack-node-externals": "^1.7.2"
  }
}
